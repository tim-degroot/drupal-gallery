<table id="subjects" class="table" style="width:100%">
    <thead class="thead-dark">
        <tr>
            <th>Type</th>
            <th>Date</th>
            <th>Questions</th>
            <th>Answers</th>
        </tr>
    </thead>
    <tbody>
        {% for exam in exams %}
        <tr>
            <th scope="row">{{ exam.type }}</td>
            <td>{{ exam.date }}</td>
            <td>{{ exam.questions }}</td>
            <td>{{ exam.answers }}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>