<div class="row justify-content-center align-items-center" id="masonry-grid">
  {% for url in images %}
    <div class="col-md-4 col-sm-6 mb-3 masonry-item d-flex justify-content-center align-items-center">
      <a data-fslightbox="gallery" href="{{ url }}">
        <img src="{{ url }}" class="img-thumbnail" loading="lazy">
      </a>
    </div>
  {% endfor %}
</div>